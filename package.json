{
  "name": "dbdr-backend",
  "version": "1.0.0",
  "description": "Backend for the CS 348 Database Doctor project.",
  "main": "index.js",
  "scripts": {
    "test": "npx eslint . --ext .ts",
    "startdb": "docker-compose up -d",
    "resetdb": "bash ./scripts/resetdb.sh",
    "gendb": "bash ./scripts/gendb.sh",
    "db": "bash ./scripts/db.sh",
    "start": "node dist/index.js",
    "build": "npx tsc",
    "watch": "npx tsc --watch & npx nodemon dist/index.js && fg",
    "dev": "npm run build; npm run watch",
    "postinstall": "npx prisma generate",
    "seed": "npx ts-node ./seed/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/database-doctor/backend.git"
  },
  "author": "Malav Mehta, Adrian Davila, Jason Du, Aarush Khurana",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/database-doctor/backend/issues"
  },
  "homepage": "https://github.com/database-doctor/backend#readme",
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/dotenv": "^8.2.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.4.2",
    "@types/node-sql-parser": "^1.1.0",
    "@types/pg": "^8.10.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.44.0",
    "nodemon": "^3.0.1",
    "pg": "^8.11.1",
    "prettier": "3.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@prisma/client": "^4.14.1",
    "apollo-server-express": "^3.12.0",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.14.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "graphql": "^16.7.1",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.22.2",
    "graphql-type-json": "^0.3.2",
    "jsonwebtoken": "^9.0.1",
    "node-sql-parser": "^4.8.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^2.0.0-beta.1",
    "typegraphql-prisma": "^0.26.0"
  }
}
